Host *
	HashKnownHosts yes

# Fairfax GitHub
# usage example: fairfaxgithub:ff-jbarros/ff-content.git
Host fairfaxgithub
	HostName github.com
	User git
	IdentityFile ~/dotfiles/public-keys/ntfairfaxdev_rsa.pub
	IdentitiesOnly yes

# Azure Repos
Host ssh.dev.azure.com
	IdentityFile ~/dotfiles/public-keys/ntfairfaxdev_rsa.pub
	IdentitiesOnly yes
